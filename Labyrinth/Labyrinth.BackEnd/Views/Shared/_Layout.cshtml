        <script src="~/Scripts/sweetalert.min.js"></script>
        <script src="~/Scripts/Addons_Library/jquery.extension.js"></script>

        @RenderSection("scripts", required: false)

        <script type="text/javascript" src="~/assets/js/Main.js"></script>

