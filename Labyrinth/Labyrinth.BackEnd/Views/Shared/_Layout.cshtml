</head>
<body class="">
    <!--Navigation Top Bar Start-->
    <nav class="navigation">
        <div class="container-fluid">
            <!--Logo text start-->
            <div class="header-logo">
                <a href="index.html" title="">
                    <h1>لوحة التحكم</h1>
                </a>
            </div>
            <!--Logo text End-->
            <div class="top-navigation">
                <!--Collapse navigation menu icon start -->
                <div class="menu-control hidden-xs">
                    <a href="javascript:void(0)">
                        <i class="fa fa-bars"></i>
                    </a>
                </div>
                <div class="search-box">
                    <ul>
                        <li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="javascript:void(0)">
                                <span class="fa fa-search"></span>
                            </a>
                            <div class="dropdown-menu  top-dropDown-1">
                                <h4>Search</h4>
                                <form>
                                    <input type="search" placeholder="what you want to see ?">
                                </form>
                            </div>
                        </li>
                    </ul>
                </div>
                <!--Collapse navigation menu icon end -->
            </div>
        </div>
    </nav>
    <!--Navigation Top Bar End-->
    <section id="main-container">
        <!--Left navigation section start-->
        <section id="left-navigation">
            <!--Left navigation user details start-->
            <div class="user-image">
                <img src="~/assets/images/demo/avatar-80.png" alt="" />
                <div class="user-online-status">
                    <span class="user-status is-online  "></span>
                </div>
            </div>
            <!--Left navigation user details end-->
            <!--Phone Navigation Menu icon start-->
            <div class="phone-nav-box visible-xs">
                <a class="phone-logo" href="/Home/Index" title="">
                    <h1>لوحة التحكم</h1>
                </a>
                <a class="phone-nav-control" href="javascript:void(0)">
                    <span class="fa fa-bars"></span>
                </a>
                <div class="clearfix"></div>
            </div>
            <!--Phone Navigation Menu icon start-->
            <!--Left navigation start-->
            <ul class="mainNav">
                <li class="active">
                    <a class="active" href="/Home/Index">
                        <i class="fa fa-dashboard"></i>
                        <span>الرئسية</span>
                    </a>
                </li>
                <li>
                    <a>
                        <i class="fa fa-magnet"></i>
                        <span>الاخبار</span>
                    </a>
                    <ul>
                        <li>
                            @Html.ActionLink("اضافة خبر جديد", "Index", "Home")
                            @Html.ActionLink("كل الاخبار المنشورة", "Index", "Home")
                            @Html.ActionLink("كل الاخبار الغير منشوره", "Index", "Home")
                            @Html.ActionLink("كل الاخبار المحذوفة", "Index", "Home")
                            @Html.ActionLink("الأخبار المجدولة", "Index", "Home")
                            @Html.ActionLink("أخبارى", "Index", "Home")
                            @Html.ActionLink("ترتيب الاخبار", "Index", "Home")
                        </li>
                    </ul>
                </li>
                <li>
                    <a>
                        <i class="fa fa-magnet"></i>
                        <span>الاقسام</span>
                    </a>
                    <ul>
                        <li>
                            @Html.ActionLink("كل الاقسام", "Index", "Section")
                            @Html.ActionLink("اضافة قسم جديد", "Add", "Section")
                            @Html.ActionLink("ترتيب الاقسام", "SectionsReorder", "Section")
                        </li>
                    </ul>
                </li>
                <li>
                    <a>
                        <i class="fa fa-magnet"></i>
                        <span>كتاب الموقع</span>
                    </a>
                    <ul>
                        <li>
                            @Html.ActionLink("كل الكتاب", "Index", "Editor")
                            @Html.ActionLink("اضافة كاتب جديد", "Add", "Editor")
                            @Html.ActionLink("ترتيب الكتاب", "EditorsReorder", "Editor")
                        </li>
                    </ul>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Home")">
                        <i class="fa fa-magnet"></i>
                        <span>التعليقات</span>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Home")">
                        <i class="fa fa-magnet"></i>
                        <span>البلوكات</span>
                    </a>
                </li>
            </ul>
            <!--Left navigation end-->
        </section>
        <section id="min-wrapper">
            <!--Left navigation section end-->
            @RenderBody()
        </section>
        <!--Layout Script start -->
        <script type="text/javascript" src="~/assets/js/color.js"></script>
